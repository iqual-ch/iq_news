{
  "name": "iqual/iq_news",
  "description": "News base module for iqual.",
  "type": "drupal-custom-module",
  "repositories": [
    {
      "type": "composer",
      "url": "https://github.com/iqual-ch/iq_news"
    }
  ],
  "require": {
    "php": "^8.1",
    "drupal/core": "^9.5 || ^10",
    "drupal/pagedesigner_block_adaptable": "^3.0 || ^4.0",
    "iqual/iq_entity_browsers": "^3.0",
    "drupal/publication_date": "^2.0",
    "drupal/scheduler": "^2.0",
    "drupal/twig_tweak": "^3.2"
  },
  "extra": {
    "patches": {
      "drupal/publication_date": {
        "[https://dgo.to/3210972]: Handle scheduler": "https://www.drupal.org/files/issues/2023-11-23/publication_date-scheduler_handler-3210972-9.patch"
      }
    }
  }
}
